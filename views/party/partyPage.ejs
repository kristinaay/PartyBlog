<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
</head>
<body>
    <p>protected page</p>
 
    <p>
        <a class="btn btn-primary btn-lg" href="/party/new">Add New Blog</a>
     </p>
     <div class="row" >
        <% parties.forEach(function(parties){ %>
            <div class="card" style="width: 18rem;">
                <img class="card-img-top" src="<%= parties.image %>" alt="Card image cap">
                <div class="card-body">
                  <h5 class="card-title"><%= parties.name %></h5>
                  <p class="card-text" ><%= parties.dest %></p>
                  <p class="card-text"><%= parties.loc %>, Cost $<%= parties.cost %> </p>
                  <a href="<%= parties.web %>" class="btn btn-primary">Website</a>
                  <p class="card-text" style = "font-style: italic" >Create by <%= parties.authorLastName %>, <%= parties.authorFirstName %></p>
                  <a class="btn btn-primary btn-lg" href="comments/<%= parties._id %>">Add Comment</a>
                </div>
              </div>
        <% }); %>
    </div>
    <a class="btn btn-primary btn-lg" href="/party/:id">User Page</a>
    <div id="card-container"></div>
    <div id="content">Content</div>
    <!-- <script src="./../../public/javascripts/main.js" async defer></script> -->
    <!-- <script src="/javascripts/main.js" async defer></script> -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
</body>
</html>